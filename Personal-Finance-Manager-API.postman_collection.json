					"// This script runs after each response",
					"console.log('Response status: ' + pm.response.code);",
					"",
					"// Basic response tests",
					"pm.test('Status code is not 500', function () {",
					"    pm.expect(pm.response.code).to.not.equal(500);",
					"});",
					"",
					"pm.test('Response time is less than 5000ms', function () {",
					"    pm.expect(pm.response.responseTime).to.be.below(5000);",
					"});",
					"",
					"// Check for authentication redirects (302) for protected endpoints",
					"if (pm.request.url.path.includes('/user') || ",
					"    pm.request.url.path.includes('/admin') || ",
					"    pm.request.url.path.includes('/transactions') || ",
					"    pm.request.url.path.includes('/budgets') || ",
					"    pm.request.url.path.includes('/goals') || ",
					"    pm.request.url.path.includes('/notifications')) {",
					"    ",
					"    if (pm.response.code === 302) {",
					"        pm.test('Redirected to login for protected endpoint', function () {",
					"            pm.expect(pm.response.headers.get('Location')).to.include('/login');",
					"        });",
					"    }",
					"}"
				]
			}
		}
